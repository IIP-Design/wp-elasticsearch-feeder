    "@wordpress/compose": "^3.25.2",
    "@wordpress/data": "^4.27.2",
    "@wordpress/edit-post": "^3.27.2",
    "@wordpress/i18n": "^3.19.2",
    "@wordpress/plugins": "^2.25.2"
  },
  "devDependencies": {
    "@gpa-lab/eslint-config": "^1.4.1",
    "@wordpress/scripts": "^14.1.1",
    "eslint": "^7.24.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IIP-Design/wp-elasticsearch-feeder.git"
